{
  "path": true,
  "multipass": true,
  "js2svg": {
    "pretty": true,
    "indent": 2
  },
  "plugins": [
    {
      "name": "preset-default",
      "params": {
        "overrides": {
          "removeViewBox": false
        }
      }
    },
    {
      "name": "removeAttrs",
      "params": {
        "attrs": "xmlns"
      }
    },
    "mergePaths",
    "cleanupListOfValues",
    "cleanupIDs",
    "sortAttrs",
    "removeDimensions",
    "removeUnusedNS",
    "removeXMLNS"
  ]
}
